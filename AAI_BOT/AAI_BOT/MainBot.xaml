<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AAI_BOT"
        xmlns:Properties="clr-namespace:AAI_BOT.Properties" x:Class="AAI_BOT.MainBot"
        mc:Ignorable="d"
        Title="MainBot" Background="#FF16262C" WindowState="Maximized">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="435*"/>
            <RowDefinition Height="71*"/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Button Content="Button" x:Name="Second" HorizontalAlignment="Stretch" VerticalAlignment="Bottom" Grid.Column="1"  Height="56" Click="AnswerBad" BorderThickness="2"/>
            <Button Content="Button" x:Name="First" HorizontalAlignment="Stretch" VerticalAlignment="Bottom" Grid.Column="0"  Height="56" Click="Answer" BorderThickness="2"/>
        </Grid>
        <ScrollViewer VerticalScrollBarVisibility="Auto">
            <Grid Name="la"/>

        </ScrollViewer>
    </Grid>
</Window>
